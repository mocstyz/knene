# 环境变量配置示例文件
# 复制此文件为 .env 并填入实际值
# @author mosctz
# @version 1.0

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=knene_db
DB_USERNAME=root
DB_PASSWORD=password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Elasticsearch配置
ES_URIS=http://localhost:9200
ES_USERNAME=
ES_PASSWORD=

# RocketMQ配置
ROCKETMQ_NAME_SERVER=localhost:9876

# MinIO配置
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=knene-resources
MINIO_SECURE=false

# JWT配置
JWT_SECRET=mySecretKey123456789
JWT_EXPIRATION=86400
JWT_REFRESH_EXPIRATION=604800

# OAuth2配置
OAUTH2_ENABLED=false
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# 邮件配置
MAIL_ENABLED=false
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_FROM=noreply@knene.com

# 支付配置
ALIPAY_ENABLED=false
ALIPAY_APP_ID=
ALIPAY_PRIVATE_KEY=
ALIPAY_PUBLIC_KEY=
WECHAT_PAY_ENABLED=false
WECHAT_APP_ID=
WECHAT_MCH_ID=
WECHAT_API_KEY=

# 外部API配置
TMDB_API_KEY=
GA4_MEASUREMENT_ID=
GA4_API_SECRET=
HOTJAR_SITE_ID=

# 缓存配置
MULTI_LEVEL_CACHE_ENABLED=true
CACHE_WARMUP_ENABLED=true
CACHE_REFRESH_ENABLED=true

# 安全配置
CSRF_ENABLED=true
CORS_ENABLED=true
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
REMEMBER_ME_ENABLED=true
PASSWORD_POLICY_ENABLED=true
LOGIN_CAPTCHA_ENABLED=true

# 监控配置
TRACING_ENABLED=true
ZIPKIN_BASE_URL=http://localhost:9411
TRACING_SAMPLE_RATE=0.1
DATASOURCE_MONITOR_ENABLED=true

# 文件存储配置
STORAGE_PATH=./uploads

# 爬虫配置
CRAWLER_ENABLED=true

# 限流配置
RATE_LIMIT_ENABLED=true

# 服务器配置
SERVER_PORT=8080

# 日志配置
LOG_LEVEL=INFO
LOG_FILE_PATH=./logs

# Flyway配置
FLYWAY_ENABLED=true

# 缓存配置
CACHE_MONITORING_ENABLED=true
CACHE_ANTI_PENETRATION_ENABLED=true
CACHE_ANTI_AVALANCHE_ENABLED=true
CACHE_CLEANUP_ENABLED=true
CACHE_DISTRIBUTED_LOCK_ENABLED=true

# 数据库读写分离配置
DATASOURCE_READ_WRITE_SPLITTING_ENABLED=false
DATASOURCE_SHARDING_ENABLED=false

# 内容安全配置
CONTENT_SAFETY_ENABLED=false

# 代理配置
PROXY_ENABLED=false

# CDN配置
CDN_ENABLED=false

# 图片处理配置
IMAGE_PROCESSOR_ENABLED=true

# 验证码配置
TURNSTILE_ENABLED=false
RECAPTCHA_ENABLED=false

# 多因子认证配置
MFA_ENABLED=false
KEY_ROTATION_ENABLED=false

# 数据权限配置
DATA_PERMISSION_ENABLED=true

# 审计配置
AUDIT_ENABLED=true

# IP白名单配置
IP_WHITELIST_ENABLED=false

# API访问控制配置
API_ACCESS_CONTROL_ENABLED=true

# 外部服务配置
TMDB_ENABLED=true
